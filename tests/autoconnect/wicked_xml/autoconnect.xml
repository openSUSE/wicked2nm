<interface origin="compat:suse:/etc/sysconfig/network/ifcfg-eth9">
  <name>eth9</name>
  <control>
    <mode>boot</mode>
  </control>
  <firewall/>
  <link/>
  <ipv4>
    <arp-verify>true</arp-verify>
  </ipv4>
  <ipv6>
    <privacy>prefer-public</privacy>
    <accept-redirects>false</accept-redirects>
  </ipv6>
</interface>
